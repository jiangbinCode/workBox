<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="500.0" prefWidth="500.0" style="-fx-background-color: #fff" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.controller.MakeController">
   <children>
      <BorderPane maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="-Infinity" minWidth="-Infinity" prefHeight="900.0" prefWidth="1500.0">
         <top>
            <HBox prefHeight="66.0" prefWidth="1480.0" BorderPane.alignment="CENTER">
               <BorderPane.margin>
                  <Insets left="20.0" top="10.0" />
               </BorderPane.margin>
               <children>
                  <TextField fx:id="filePath" />
                  <Button mnemonicParsing="false" onAction="#selectFile" text="选择文件夹">
                     <HBox.margin>
                        <Insets left="5.0" />
                     </HBox.margin>
                  </Button>
               </children>
            </HBox>
         </top>
         <left>
            <TreeView fx:id="fileTree" prefHeight="819.0" prefWidth="229.0" BorderPane.alignment="CENTER" />
         </left>
         <right>
            <VBox prefHeight="840.0" prefWidth="261.0" BorderPane.alignment="CENTER">
               <children>
                  <Separator prefWidth="200.0" />
                  <Label prefHeight="22.0" prefWidth="49.0" text="主图表">
                     <font>
                        <Font size="14.0" />
                     </font>
                  </Label>
                  <TableView fx:id="masterImgTable" prefHeight="232.0" prefWidth="261.0">
                    <columns>
                      <TableColumn fx:id="m_name" prefWidth="132.0" text="图片名字" />
                      <TableColumn fx:id="m_num" prefWidth="125.0" text="序号" />
                    </columns>
                  </TableView>
                  <Label prefHeight="22.0" prefWidth="57.0" text="选项图表">
                     <font>
                        <Font name="Arial" size="14.0" />
                     </font>
                     <VBox.margin>
                        <Insets top="5.0" />
                     </VBox.margin>
                  </Label>
                  <TableView fx:id="skuImgTable" prefHeight="283.0" prefWidth="261.0">
                    <columns>
                      <TableColumn fx:id="s_name" prefWidth="186.0" text="sku名字" />
                      <TableColumn fx:id="s_num" prefWidth="71.0" text="序号" />
                    </columns>
                  </TableView>
                  <Label prefHeight="21.0" prefWidth="68.0" text="详情图表">
                     <font>
                        <Font size="14.0" />
                     </font>
                     <VBox.margin>
                        <Insets top="7.0" />
                     </VBox.margin>
                  </Label>
                  <TableView fx:id="detailImgTable" prefHeight="253.0" prefWidth="261.0">
                    <columns>
                      <TableColumn fx:id="d_name" prefWidth="119.0" text="图片名字" />
                      <TableColumn fx:id="d_num" prefWidth="134.0" text="序号" />
                    </columns>
                  </TableView>
                  <HBox prefHeight="100.0" prefWidth="200.0">
                     <children>
                        <ImageView fx:id="previewImg" fitHeight="142.0" fitWidth="150.0" pickOnBounds="true" preserveRatio="true">
                           <HBox.margin>
                              <Insets bottom="5.0" top="5.0" />
                           </HBox.margin>
                        </ImageView>
                     </children>
                  </HBox>
               </children>
               <BorderPane.margin>
                  <Insets left="10.0" right="10.0" />
               </BorderPane.margin></VBox>
         </right>
         <center>
            <VBox prefHeight="200.0" prefWidth="100.0" BorderPane.alignment="CENTER">
               <children>
                  <Separator prefWidth="200.0" />
                  <HBox prefHeight="33.0" prefWidth="1050.0">
                     <children>
                        <CheckBox fx:id="fileCheckAll" mnemonicParsing="false" onAction="#fileCheckAll" prefHeight="30.0" prefWidth="53.0" text="全选">
                           <HBox.margin>
                              <Insets left="3.0" />
                           </HBox.margin>
                        </CheckBox>
                        <Button fx:id="tagMasterImg" mnemonicParsing="false" onAction="#tagMasterImg" prefHeight="23.0" prefWidth="95.0" text="标识为主图">
                           <HBox.margin>
                              <Insets bottom="2.0" left="5.0" top="2.0" />
                           </HBox.margin>
                        </Button>
                        <Button fx:id="tagSkuImg" mnemonicParsing="false" onAction="#tagSkuImg" text="标识为选项图">
                           <HBox.margin>
                              <Insets bottom="2.0" left="5.0" top="2.0" />
                           </HBox.margin>
                        </Button>
                        <Button fx:id="tagDetailsImg" mnemonicParsing="false" onAction="#tagDetailsImg" text="标识为详情图">
                           <HBox.margin>
                              <Insets bottom="2.0" left="5.0" top="2.0" />
                           </HBox.margin>
                        </Button>
                        <Button fx:id="tagWhiteImg" mnemonicParsing="false" onAction="#tagWhiteImg" text="标识为透明图">
                           <HBox.margin>
                              <Insets bottom="2.0" left="5.0" top="2.0" />
                           </HBox.margin>
                        </Button>
                     </children>
                  </HBox>
                  <TableView fx:id="fileTableTableView" prefHeight="814.0" prefWidth="990.0">
                    <columns>
                        <TableColumn fx:id="fCheckBox" prefWidth="75.0" text="选择" />
                      <TableColumn fx:id="path" minWidth="100.0" prefWidth="215.0" text="文件路径" />
                      <TableColumn fx:id="name" prefWidth="162.0" text="文件名" />
                        <TableColumn fx:id="fileSize" prefWidth="132.0" text="文件大小" />
                        <TableColumn fx:id="pixel" prefWidth="112.0" text="尺寸(PX)" />
                        <TableColumn fx:id="fileType" minWidth="4.0" prefWidth="102.0" text="文件类型" />
                        <TableColumn fx:id="useWay" prefWidth="211.0" text="作用于" />
                    </columns>
                     <VBox.margin>
                        <Insets />
                     </VBox.margin>
                  </TableView>
               </children>
            </VBox>
         </center>
      </BorderPane>
   </children>
</AnchorPane>
